    "caption": "DB Notebook",
    "content": "\\about\nuse armbase;\nselect * from celeb;\nselect * from snl_show;\nselect concat ('이름:', name) from celeb;\nselect name as '이름', agency as '소속사' from celeb;\nselect concat (name, ' : ', job_title) as profile from celeb;\nselect s.season, s.episode, c.name, c.job_title\nfrom celeb c, snl_show s\nwhere c.name = s.host;\nselect concat(s.season, '-', s.episode, '(', s.broadcast_date, ')') '방송정보',\nconcat(c.name, '(', c.job_title, ')') as '출연자정보'\nfrom celeb as c, snl_show s\nwhere c.name = host;\nselect concat('이름 : ', c.name, ',  ' '소속사 : ', c.agency) '연예인 정보'\nfrom celeb c\nwhere c.name like '___';\nselect concat('소속사 : ', c.agency) '소속사 정보',\nconcat('나이 : ', c.age,'(',c.sex,')') '신상정보',\nconcat(s.season,'-', s.episode,', ', '방송날짜 : ', s.broadcast_date) '출연정보'\nfrom celeb c, snl_show s\nwhere (c.agency like '__엔터테이먼트' and c.name = s.host);\nselect agency from celeb;\nselect DISTINCT agency from celeb;\nSELECT sex, job_title FROM celeb WHERE job_title like '%가수%';\nSELECT DISTINCT sex, job_title FROM celeb WHERE job_title like '%가수%';\nselect DISTINCT sex, agency from celeb ORDER BY sex, agency;\nselect sex, agency from celeb ORDER BY sex, agency;\nSELECT s.season, c.agency from celeb c, snl_show s\nwhere c.name=s.host\nSELECT DISTINCT s.season, c.agency from celeb c, snl_show s\nwhere c.name=s.host\nselect * from celeb limit 3;\nselect * from celeb order by age limit 5;\nselect * from celeb \nwhere sex='M' ORDER BY age DESC LIMIT 2;\nselect concat('SNL 시즌 ', s.season, ' 에피소드 ', s.episode, ' 호스트 ', c.name) 'SNL 방송정보',\nconcat(c.age) 'age'\nfrom celeb c, snl_show s\nwhere c.name=s.host and c.sex='M'\nORDER BY c.age DESC, s.broadcast_date DESC LIMIT 2;\n\n",
